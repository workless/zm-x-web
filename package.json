{
  "private": true,
  "name": "zimbra-client",
  "description": "Zimbra X Web Interface",
  "main": "build/bundle.js",
  "module": "src/index.js",
  "scripts": {
    "build": "webpack -p && npm run build:netlify",
    "build:dev": "webpack",
    "build:netlify": "node packages/static-app-to-netlify",
    "build:analyze": "WEBPACK_ANALYZE_REPORT=true webpack -p",
    "eslint": "eslint src --ext .js,.graphql --ignore-pattern **/schema.graphql",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint -- --fix",
    "packages:install": "node node_modules/recursive-install --skip-root",
    "postinstall": "npm run -s packages:install",
    "serve": "node ./scripts/superstatic-https.js",
    "start": "node ./scripts/start.js",
    "test": "npm run lint && npm run test:unit && npm run test:no-synacor-npm-refs && npm run build && npm run test:e2e:smoke",
    "test:unit": "karma start tests/karma.conf.js --single-run",
    "test:watch": "JUNIT=false COVERAGE=false npm run test:unit -- --no-single-run",
    "test:e2e:smoke": "HTML_REPORT_PATH='test-reports/testcafe-smoke/' testcafe -c 5 chrome tests/e2e/testcafe/ -e -q --app 'npm run serve' --app-init-delay 5000 -T \"Smoke\" -r html-testrail,xunit:test-reports/testcafe-smoke/xunit.xml",
    "test:no-synacor-npm-refs": "node ./scripts/synacor-npm-refs.js"
  },
  "keywords": [
    "email",
    "zimbra"
  ],
  "repository": "Zimbra/zm-x-web",
  "contributors": [
    "Bill Neff <bneff@synacor.com>",
    "Peter Lenahan <peter.lenahan@synacor.com>"
  ],
  "license": "GPL-3.0",
  "devDependencies": {
    "axe-testcafe": "^1.1.0",
    "babel-eslint": "=8.2.1",
    "chalk": "^2.0.1",
    "eslint": "^4.15.0",
    "eslint-config-synacor": "^3.0.1",
    "eslint-plugin-graphql": "^1.5.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "file-matcher": "^1.1.0",
    "graphql-tag": "^2.6.1",
    "html-validator": "^2.2.3",
    "if-env": "^1.0.0",
    "js2xmlparser": "^3.0.0",
    "karma": "^2.0.0",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-coverage": "^1.1.1",
    "karma-junit-reporter": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
